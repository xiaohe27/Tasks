
.tree
{
	background: #F1F1F1;
	cursor: default;
	padding-top: 2px;
}

.treeItem
{
	height: 19px;
}

.treeBullet
{
	font: 70% Verdana;
	width: 24px;
	height: 19px;
	padding-left: 8px;
}

.treeLabel
{
	height: 19px;
	border-style: solid;
	border-width: 1px;
	border-color: #F1F1F1;
	padding: 1px 2px 0px 2px;
	cursor: hand;
}

.treeLabel a
{
	text-decoration: none;
	color: Black;
}

.treeContainer
{
	padding-left: 21px;
}

.treeBox
{
	width: 11px;
	height: 11px;
	overflow: hidden;
	margin-top: 4px;
	border: solid 1px black;
	background: white;
	text-align: center;
	cursor: hand;
}

.treePlus
{
	font: 9px Verdana;
	position: relative;
	top: -2px;
}

.treeMinus
{
	font: 15px Verdana;
	line-height: 15px;
	position: relative;
	top: -4px;
	padding-left: 1px;
}

.treeDot
{
	font: 9px Verdana;
	position: relative;
	top: -4px;
}
			.treeLabel
			{
				font: 70% Verdana;
			}
        body
{
	margin: 0px;
}
